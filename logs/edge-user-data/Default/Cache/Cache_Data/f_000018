    let filtered = [...contas];
    if (filters.tipo !== "todos")
      filtered = filtered.filter((c) => c.tipo === filters.tipo);
    if (filters.status !== "todos")
      filtered = filtered.filter((c) => getStatus(c) === filters.status);
