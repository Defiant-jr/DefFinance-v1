        if (item.tipo === "Entrada") {
          fluxo[dayIndex].receber += item.valor;
          fluxo[dayIndex].details.receber.push(item);
        } else {
          fluxo[dayIndex].pagar += item.valor;
          fluxo[dayIndex].details.pagar.push(item);
        }
      }
    });
